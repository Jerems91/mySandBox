<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="CustomMVC" packages="">
	<Appenders>
		<Console name="console" target="SYSTEM_OUT">
			<PatternLayout pattern="[%date][%level][%logger][%method] - %message%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Root level="debug">
			<AppenderRef ref="console"/>
		</Root>
	</Loggers>
</Configuration>
